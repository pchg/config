#:/bin/sh
aa=`tempfile`
bb=`tempfile`
echo "1st file, finish with Ctrl-D:  =========="
cat > $aa
echo "2nd file, finish with Ctrl-D:  =========="
cat > $bb
#echo "3rd file, , finish with Ctrl-D: =========="
#cat > /tmp/cc
sed -i "s/  */ /g" $aa
sed -i "s/  */ /g" $bb
#sed -i "s/  */ /g" /tmp/cc
vimdiff $aa $bb #/tmp/cc

